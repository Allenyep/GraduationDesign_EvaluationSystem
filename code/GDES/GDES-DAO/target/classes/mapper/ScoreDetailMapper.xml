<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdes.GDES.dao.ScoreDetailDao">
    <resultMap id="scoredetailmap" type="ScoreDetail">
        <id column="id_t" property="id_t"/>
        <id column="id_s" property="id_s"/>
        <id column="id_er" property="id_er"/>
        <id column="id_q" property="id_q"/>
        <id column="id_ap" property="id_ap"/>
        <result column="score_sd" property="score_sd"/>
        <result column="time_sd" property="time_sd"/>
    </resultMap>

    <select id="getScoreDetailListByAbilityId" parameterType="LatestAbilityScore" resultMap="scoredetailmap">
        SELECT * FROM scoredetail WHERE id_ap = #{id_ap} AND id_s = #{id_s}
    </select>
</mapper>