<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdes.GDES.dao.AbilityPointDao">
    <resultMap id="abilitypointmap" type="AbilityPoint">
        <id column="id_ap" property="id_ap"/>
        <result column="name_ap" property="name_ap"/>
        <result column="aspect_ap" property="aspect_ap"/>
        <result column="class_ap" property="class_ap"/>
    </resultMap>

    <select id="getAbilityPointById" parameterType="Integer" resultMap="abilitypointmap">
        SELECT * FROM abilitypoint WHERE id_ap = #{id_ap}
    </select>
</mapper>